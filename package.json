{
  "name": "slides",
  "version": "1.0.0",
  "private": true,
  "description": "My slides",
  "homepage": "https://collides.netlify.app",
  "author": "Colder",
  "scripts": {
    "build": "astro build",
    "dev": "astro dev",
    "preview": "astro preview",
    "start": "astro dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "prettier": {
    "pluginSearchDirs": [
      "."
    ],
    "plugins": [
      "prettier-plugin-astro",
      "prettier-plugin-jsdoc",
      "prettier-plugin-packagejson",
      "prettier-plugin-organize-imports"
    ],
    "singleQuote": true,
    "overrides": [
      {
        "files": "**/*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  },
  "dependencies": {
    "@astrojs/react": "^1.1.3",
    "@astrojs/tailwind": "^2.0.1",
    "astro": "^1.2.6",
    "daisyui": "^2.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reveal.js": "^4.3.1",
    "tailwindcss": "^3.1.8"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "netlify-cli": "^11.8.0",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.5.4",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-organize-imports": "^3.1.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "typescript": "^4.8.3"
  },
  "packageManager": "pnpm@7.12.0"
}
